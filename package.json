    "jest": "^26.6.3",
    "msw": "^0.26.2",
    "prettier": "^2.2.1",
    "timekeeper": "^2.2.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "cross-fetch": "^3.0.6",
